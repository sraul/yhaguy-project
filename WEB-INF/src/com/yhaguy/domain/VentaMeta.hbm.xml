<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]>

<hibernate-mapping>

	<class name="com.yhaguy.domain.VentaMeta" table="ventameta">

		&hibernate-domain; 

		<property name="periodo"/>
		<property name="enero"/>
		<property name="febrero"/>
		<property name="marzo"/>
		<property name="abril" />
		<property name="mayo" />		
		<property name="junio" />
		<property name="julio" />	
		<property name="agosto" />
		<property name="setiembre" />
		<property name="octubre" />
		<property name="noviembre" />
		<property name="diciembre" />
		
	</class>
</hibernate-mapping>
