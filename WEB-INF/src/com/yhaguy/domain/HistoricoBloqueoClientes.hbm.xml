<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]>

<hibernate-mapping>

	<class name="com.yhaguy.domain.HistoricoBloqueoClientes" table="historico_bloqueo_clientes">

		&hibernate-domain; 
		
		<property name="fecha" />
		<property name="vencimiento" />
		<property name="cliente" />
		<property name="numeroFactura" />
		<property name="motivo" />
		<property name="diasVencimiento" />
					
	</class>
</hibernate-mapping>
