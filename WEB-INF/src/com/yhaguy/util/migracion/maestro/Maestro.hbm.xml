<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]
        >

<hibernate-mapping>

	<class name="com.yhaguy.util.migracion.maestro.Maestro" table="maestro">

		&hibernate-domain;
        <property name="codigoProveedor" />
        <property name="codigoOriginal" />
		<property name="descripcion" />
		<property name="datoExtra"  type="text"  />
		<property name="marca_Aplicacion" />
		<property name="peso" />
		<property name="imagen" />
        <property name="marca" />
        <property name="parte" />
        <property name="familia" />
        <property name="linea" />
        <property name="codigoBarra" />
       

	</class>
</hibernate-mapping>
